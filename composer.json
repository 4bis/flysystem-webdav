{
    "name": "4bis/flysystem-webdav",
    "description": "Flysystem adapter for WebDAV",
    "license": "MIT",
    "authors": [
        {
            "name": "Frank de Jonge",
            "email": "info@frenky.net"
        }
    ],
    "require": {
        "php": ">=5.6",
        "league/flysystem": "~1.0",
        "4bis/dav": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "mockery/mockery": "~1.2"
    },
    "autoload": {
        "psr-4": {
            "League\\Flysystem\\WebDAV\\": "src"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:4bis/dav.git"
        },
        {
            "type": "vcs",
            "url":  "git@github.com:4bis/http.git"
        }
    ]
}
